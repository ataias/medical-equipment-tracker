{
  "name": "@medical-equipment-tracker/authentication",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -e js,yaml src/index.js",
    "db:create": "cross-env NODE_ENV=test node scripts/setupTestDb.js",
    "db:migrate": "cross-env NODE_ENV=test sequelize db:migrate",
    "db:reset": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "db:seed": "cross-env NODE_ENV=test sequelize db:seed:all",
    "start": "cross-env NODE_ENV=production node src/index.js",
    "test": "jest --coverage --detectOpenHandles",
    "test:clear": "jest --clearCache",
    "test:run": "cross-env NODE_ENV=test npm-run-all -s db:create db:reset db:migrate db:seed test"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "9.1.0",
    "@medical-equipment-tracker/mailer": "1.0.0",
    "@medical-equipment-tracker/validator": "1.0.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "dotenv-cli": "3.1.0",
    "express": "4.17.1",
    "express-jwt": "6.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash.get": "4.4.2",
    "lodash.intersection": "4.4.0",
    "lodash.pick": "4.4.0",
    "lodash.uniq": "4.5.0",
    "morgan": "1.10.0",
    "pg": "8.2.1",
    "sequelize": "6.2.4",
    "uuid": "8.2.0",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0"
  },
  "devDependencies": {
    "eslint": "7.4.0",
    "eslint-plugin-node": "11.1.0",
    "jest": "26.1.0",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "pgtools": "0.3.0",
    "sequelize-cli": "6.2.0",
    "supertest": "4.0.2",
    "swagger-ui-express": "4.1.4",
    "yamljs": "0.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
